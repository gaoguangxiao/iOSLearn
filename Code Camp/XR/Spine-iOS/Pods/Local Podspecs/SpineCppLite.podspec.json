{
  "name": "SpineCppLite",
  "version": "4.2.0",
  "summary": "Spine runtimes for iOS.",
  "description": "Spine runtimes for iOS.",
  "homepage": "https://esotericsoftware.com",
  "authors": {
    "Esoteric Software LLC  ": "https://esotericsoftware.com"
  },
  "license": {
    "file": "LICENSE"
  },
  "platforms": {
    "ios": "13.0"
  },
  "source": {
    "git": "https://github.com/esotericsoftware/spine-runtimes.git",
    "branch": "4.2"
  },
  "source_files": [
    "spine-cpp/spine-cpp/**/*.{h,cpp}",
    "spine-cpp/spine-cpp-lite/*.{h,cpp}"
  ],
  "module_map": "spine-cpp/spine-cpp-lite/module.modulemap",
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/SpineCppLite/spine-cpp/spine-cpp/include\" \"$(PODS_ROOT)/SpineCppLite/spine-cpp/spine-cpp-lite\"",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++11",
    "CLANG_CXX_LIBRARY": "libc++"
  }
}
